<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kfoodbox.article.repository.CommunityArticleRepository">
    <select id="findCommunityArticleById" parameterType="long" resultType="kfoodbox.article.entity.CommunityArticle">
        SELECT *
        FROM `kfoodbox`.`community_article`
        WHERE `id` = #{id}
    </select>
</mapper>
